+#!/bin/bash
+
+set -euo pipefail
+IFS=$'\n\t'
+
+if [[ ! -f "rootmaindirAETHER.proj" ]]; then
+  echo "ðŸ§¨ Error: rootmaindirAETHER.proj not found in the current directory!"
+  exit 1
+fi
+
+current_branch=$(git symbolic-ref --short HEAD)
+if [[ "$current_branch" != "main" ]]; then
+  echo "ðŸš¨ You are currently on branch '$current_branch'."
+  read -p "ðŸŒ± Do you want to switch to 'main' before continuing? (y/n): " switch_answer
+  if [[ "$switch_answer" =~ ^[Yy]$ ]]; then
+    git checkout main
+  else
+    echo "ðŸ›‘ Please switch to 'main' to generate patch series correctly."
+    exit 1
+  fi
+fi
+
+if ! git diff --quiet || ! git diff --cached --quiet; then
+  echo "âš ï¸ Warning: You have uncommitted changes."
+  read -p "ðŸ’¾ Do you want to commit them before proceeding with the rebase? (y/n): " commit_answer
+  if [[ "$commit_answer" =~ ^[Yy]$ ]]; then
+    echo "ðŸ“¦ Adding all files (including untracked)..."
+    git add -A
+    read -p "ðŸ“ Enter your commit message: " commit_msg
+    git commit -m "$commit_msg"
+  else
+    echo "ðŸ¤” You chose not to commit."
+    read -p "ðŸ§º Do you want to stash your changes instead? (y/n): " stash_answer
+    if [[ "$stash_answer" =~ ^[Yy]$ ]]; then
+      echo "ðŸ“¦ Stashing your changes..."
+      git stash push -m "Pre-rebase stash"
+    else
+      echo "ðŸ›‘ Please commit or stash your changes before proceeding."
+      exit 1
+    fi
+  fi
+fi
+
+echo "ðŸ“¡ Fetching latest changes from origin..."
+git fetch origin
+
+BASE_COMMIT=$(git merge-base HEAD origin/main)
+commits_ahead=$(git log "$BASE_COMMIT"..HEAD --oneline)
+
+if [[ -z "$commits_ahead" ]]; then
+  echo "âœ… Your branch is up to date with no new commits to patch!"
+  exit 0
+fi
+
+echo "ðŸ” Rebasing on origin/main..."
+git rebase origin/main
+
+if git stash list | grep -q "Pre-rebase stash"; then
+  echo "ðŸ“¥ Applying stashed changes safely (with apply)..."
+  git stash apply
+  echo "ðŸ§¹ If all looks good, run 'git stash drop' to remove it."
+fi
+
+echo "ðŸ§µ Generating patch series..."
+git format-patch "$BASE_COMMIT" --cover-letter --stdout > new_patch_series.patch
+
+if [[ ! -s new_patch_series.patch ]]; then
+  echo "ðŸ“­ The patch series is empty. Nothing to save."
+  exit 0
+fi
+
+echo "ðŸŽ‰ Your patch series is ready: new_patch_series.patch"
